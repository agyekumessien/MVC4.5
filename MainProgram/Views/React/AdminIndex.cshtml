<body class="skin-blue">
    <div class="wrapper">

        <header class="main-header">
            <!-- Logo -->
            <a href="Index#" class="logo"><b>Admin</b>Dashboard</a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
               
               
                <span>
                    
                    <a href="~/Admin/Admin/Dashboard"><i class="fa fa-circle-o"></i> AdminDashboard
                            <img src="~/Content/images/icons-png/home5.jpg" style="border-radius: 50%; margin-top:8px; margin-left:15px">
                        </a>
                   </span>

            </nav>
        </header>

        <div class="content-wrapper" style="width: 15%; margin-left: 0px; width:100%; top:0px;">


            <section class="content-header" style="margin-left:0px; top:2px;">

                <ol class="breadcrumb">
                    <li>
                        <a href="#">
                            <i class="headerA"></i>
                        </a>
                    </li>
                    <li class="active"></li>
                </ol>
            </section>

           
           
            <!--MID Sidebar Main content-->
            <div class="Mid-sidebar" style="margin-right:10px; padding-right:0px; text-align:center">
                <div class="panel panel-default" style="width: 100%; margin-left: 0px;">
                    <strong style="font-size:x-large"><b>Top questions</b></strong>



                    <h2>Post</h2>

                    <p>
                        @Html.ActionLink("New Post", "Create", "React")
                    </p>
                    <hr />
                    <fieldset>
                        @using (Html.BeginForm("Index", "React", FormMethod.Get))
                        {
                            <p>
                                Find by name: @Html.TextBox("SearchString", ViewBag.CurrentFilter as string)
                                <input type="submit" value="Search" />
                            </p>
                        }
                        <table class="table">
                            <tr>
                                <th>
                                    @Html.ActionLink("React", "Index", new { sortOrder = ViewBag.NameSortParm, currentFilter = ViewBag.CurrentFilter })
                                </th>
                                <th>
                                    First Name
                                </th>
                                <th>
                                    @Html.ActionLink("PostDate", "Index", new { sortOrder = ViewBag.DateSortParm, currentFilter = ViewBag.CurrentFilter })
                                </th>
                                <th></th>
                            </tr>


                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ImagePath)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ReactText)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.ReactTitle)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.PostDate)
                                    </td>
                                    <td>
                                        @Html.ActionLink("Edit", "Edit", new { id = item.ReactId }) |
                                        @Html.ActionLink("Details", "Details", new { id = item.ReactId }) |
                                        @Html.ActionLink("Delete", "Delete", new { id = item.ReactId })
                                    </td>
                                </tr>
                            }

                        </table>
                        <br />
                        Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) of @Model.PageCount

                        @Html.PagedListPager(Model, page => Url.Action("Index",
    new { page, sortOrder = ViewBag.CurrentSort, currentFilter = ViewBag.CurrentFilter }))




                    </fieldset>
                </div><!--End Of MID-->


                <footer class="main-footer">
                    <div class="pull-right hidden-xs">
                        <b>Version</b> 2.0
                    </div>
                    <strong>Copyright &copy; 2014-2015 <a href="http://almsaeedstudio.com">Almsaeed Studio</a>.</strong> All rights reserved.
                </footer>





            </div>
        </div>


