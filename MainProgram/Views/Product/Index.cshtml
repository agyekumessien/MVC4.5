<!--Menu drop down-->
<script>
    $(document).ready(function () {
        $('.nav li> .dropdown-menu').parent().hover(function () {
            var Model = $(this).children('.dropdown-menu');
            if ($(Model).is(':hidden')) {
                $(Model).slideDown(400);
                // $(Model).delay(2000);
                // $('.nav li').slideDown("slow");
            } else {
                $(Model).slideUp(400);
            }
        });
    })

</script>


